n      horseFields: [{\n        key: \"scratchButton\",\n        label: \"\"\n      }, {\n        key: \"programNumber\",\n        label: \"#\"\n      }, {\n        key: \"mlodds\",\n        \"label\": \"ML\",\n        sortable: true\n      }, \"name\", {\n        key: \"daysSinceLastRace\",\n        label: \"l/r\",\n        tdClass: this.highlightDaysSince\n      }, {\n        key: \"furlongDays\",\n        label: \"FD\",\n        formatter: this.format2Places,\n        sortable: true\n      }, {\n        key: \"style\",\n        label: \"Style\",\n        tdClass: this.highlightPaceAdvantage\n      }, {\n        key: \"classRating\",\n        label: \"Class\",\n        formatter: this.format2Places,\n        sortable: true,\n        tdClass: this.highlightMax\n      }, {\n        key: \"speedRating\",\n        label: \"Speed\",\n        formatter: this.format2Places,\n        sortable: true,\n        tdClass: this.highlightMax\n      }, {\n        key: \"arating\",\n        label: \"AR\",\n        formatter: this.format2Places,\n        sortable: true,\n        tdClass: this.highlightMax\n      }, {\n        key: \"aratingClass\",\n        label: \"AR Cl\",\n        formatter: this.format2Places,\n        sortable: true,\n        tdClass: this.highlightMax\n      }, {\n        key: \"aratingForm\",\n        label: \"AR F\",\n        formatter: this.format2Places,\n        sortable: true,\n        tdClass: this.highlightMax\n      }, {\n        key: \"aratingConnections\",\n        label: \"AR Co\",\n        formatter: this.format2Places,\n        sortable: true,\n        tdClass: this.highlightMax\n      }, {\n        key: \"e1Avg\",\n        label: \"E1\",\n        formatter: this.formatInt,\n        sortable: true,\n        tdClass: this.highlightMax\n      }, {\n        key: \"e2Avg\",\n        label: \"E2\",\n        formatter: this.formatInt,\n        sortable: true,\n        tdClass: this.highlightMax\n      }, {\n        key: \"turnTime\",\n        label: \